;----------------------------------------------------------------------;
;ネオジオバトルコロシアム  クルクルタウンステージ   byJAM
;----------------------------------------------------------------------;
[Info]
name= "NBC - Kurukuru"    ;MUGEN上に表示される名前

;----------------------------------------------------------------------;
;■カメラ
[Camera]
startx         = 0                ;一番最初のｶﾒﾗ位置(X軸)
starty         = 0　　　　　　　　;一番最初のｶﾒﾗ位置(Y軸)
boundleft      = -215              ;左画面端の距離
boundright     = 215              ;右画面端の距離
boundhigh      = -18               ;上にｽｸﾛｰﾙする距離。0だとｽｸﾛｰﾙしない
boundlow       = .05　　　　   　　;下画面端の距離
verticalfollow = .08               ;ｼﾞｬﾝﾌﾟした時、ｽﾃｰｼﾞ全体が上下する強さ
tension        = 60               ;ﾄﾞｯﾄ数だけ画面端に行くと、ｽｸﾛｰﾙする

;-----------------------------------------------------------------------;
;■キャラ
[PlayerInfo]
;--- Player 1 ---
p1startx   = -80                  ;1Pキャラの最初の立ち位置X軸
p1starty   = 0                    ;1Pキャラの最初の立ち位置Y軸
p1startz   = 0                    ;使用しない
p1facing   = 1                    ;1Pキャラの最初の向き。1だと、左向き 
;--- Player 2 ---
p2startx   = 80
p2starty   = 0
p2startz   = 0
p2facing   = -1
;--- Common ---　　　　         　;変える必要無し
leftbound  = -1000          
rightbound =  1000           
topbound   =  0           
botbound   =  0            

;---------------------------------------------------------------------------------;
;大きさ
[Scaling]               ;ここの項目は特殊なステージ作る時以外はいじらなくて良い
topz     = 0            ;
botz     = 50        ;
topscale = 1            ;キャラの縮尺を設定できる（非推奨）
botscale = 1.2          ;

;-------------------------------------------------------------------------------------;
;座標
[Bound]                   
screenleft  = 15                  ;左画面端の壁の存在距離
screenright = 15                  ;右画面端の壁の存在距離

;-----------------------------------------------------------------------------------------;
;情報
[StageInfo]
zoffset  = 218            
autoturn = 1                       ;ｷｬﾗが自動的に振り向くか(1で振り向く。基本的に1
resetBG  = 0                       ;ﾗｳﾝﾄﾞ終了後ｱﾆﾒをﾘｾｯﾄするか(1でon)

;-------------------------------------------------------------------------------------;
;影
[Shadow]
color = 100, 100, 100              ;影のｶﾗｰ    
yscale = .1                        ;影のｻｲｽﾞ             
reflect = 0                        ;影の反射            

;----------------------------------------------------------------------------------------;
;音声
[Music]
bgmusic = stages/187.mp3
bgvolume = 255                     ;BGMの音量

;-------------------------------------------------------------------------------------;
;画像ファイルの指定
[BGdef]
spr=stages/nbc-kurukuru.sff         ;ｽﾃｰｼﾞの画像ﾌｧｲﾙの指定
debugbg = 0  　　　　　　　　      ;1にすると、画像が無い部分に、残像がつかない。

;====================================================================
;ステージ画像位置等
;====================================================================
;メイン背景足場チカチカ
[BG 00]
type        = Anim   　 ;画像表示タイプ。Normal=静止画、Anim=アニメ画、Parallax=立体静止画（一点透視図法）
spriteNo    = 0,0       ;画像ファイル（*.sff）の画像を表示。Animの時は必要無し
layerNo     = 0         ;レイヤー属性。0=奥、1=手前に表示
start       = -1,-1     ;軸位置。このdefファイルで設定した軸位置から数える
delta       = 1.0,1.0   ;キャラが動いた時のスクロールスピード。小数点以下可能
trans       = None      ;透過処理。Add=加算処理（輝く半透明）,Add1=半透明,Sub=減算処理（影の半透明）,None=透過しない
mask        = 1         ;画像の背景色を透過させるかどうか。1=Yes,0=No
tile        = 0,0       ;タイル化して同じ画像を複数表示させるかどうか（それぞれX,Y）
tilespacing = 0,0       ;タイル化した画像同士の間隔。
ActionNo    = 01         ;使用するアニメのナンバー。
velocity    = 0,0       ;画像の移動速度

